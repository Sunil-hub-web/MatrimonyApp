            android:background="@color/white" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/ten">

            <ImageView
                android:id="@+id/image_bluckOrder"
                android:layout_width="@dimen/twenty_five"
                android:layout_height="@dimen/twenty_five"
                android:layout_centerVertical="true">

            </ImageView>

            <TextView
                android:id="@+id/nav_BulkOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/ten"
                android:layout_toRightOf="@id/image_bluckOrder"
                android:text="Bulk Order"
                android:textColor="@color/back_bg2"
                android:textSize="@dimen/edittextsize"
                android:fontFamily="@font/poppins_medium">

            </TextView>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/text1"
            android:layout_marginTop="@dimen/ten"
            android:background="@color/white" /-->>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/ten">

            <ImageView
                android:id="@+id/image_logout"
                android:layout_width="@dimen/twenty_five"
                android:layout_height="@dimen/twenty_five"
                android:src="@drawable/logout"
                app:tint="@color/back_bg2"
                android:layout_centerVertical="true">

            </ImageView>

            <TextView
                android:id="@+id/nav_Logout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/ten"
                android:layout_toRightOf="@id/image_logout"
                android:text="Logout"
                android:textColor="@color/back_bg2"
                android:textSize="@dimen/twenty"
                android:fontFamily="@font/poppins_bold">

            </TextView>

        </RelativeLayout>

    </LinearLayout>


</RelativeLayout>